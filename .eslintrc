{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "globals": {
    "elements": 1,
    "YT": 1,
    "_interval": 1
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "no-debugger": 1,
    "eqeqeq": 1,
    "key-spacing": 0,
    "new-cap": 0,
    "strict": [
      "error",
      "global"
    ],
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,
    "eol-last": 0,
    "quotes": 0,
    "semi": 0,
    "space-infix-ops": 0,
    "no-trailing-spaces": 0,
    "no-multi-spaces": 0,
    "no-alert": 0,
    "no-undef": 2,
    "no-unused-vars": [
      1,
      {
        "vars": "local",
        "args": "none"
      }
    ]
  }
}